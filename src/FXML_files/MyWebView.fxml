<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MyWebViewController">
   <children>
      <WebView fx:id="myWebView" prefHeight="423.0" prefWidth="715.0" />
      <Button fx:id="closeButton" mnemonicParsing="false" onAction="#closeAction" style="-fx-background-color: null;" text="Close" textFill="WHITE">
         <font>
            <Font name="Ravie" size="14.0" />
         </font>
         <effect>
            <DropShadow spread="0.87" />
         </effect>
      </Button>
   </children>
</AnchorPane>
